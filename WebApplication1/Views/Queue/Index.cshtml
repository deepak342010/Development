@model IEnumerable<TblQueue>
@{
    ViewData["Title"] = "Index";
}
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Process Name")
            </th>
            <th>
                @Html.DisplayName("Queue Name")
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VC_PROCESS_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VC_QUEUE_NAME)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-queueId="@item.IN_QUEUE_ID">Edit</a>
                    <a asp-action="Details" asp-route-queueId="@item.IN_QUEUE_ID">Details</a>
                    <a asp-action="Delete" asp-route-queueId="@item.IN_QUEUE_ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
