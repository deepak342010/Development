@model IEnumerable<WebApplication1.Models.TblProcess> 
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Process Name")
            </th>
            <th>
                @Html.DisplayName("Last Sequence")
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VC_PROCESS_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IN_LAST_SEQ)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IN_PROCESS_ID">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.IN_PROCESS_ID">Details</a>
                    <a asp-action="Delete" asp-route-id="@item.IN_PROCESS_ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
